{
  "name": "headway-frontend",
  "version": "0.0.1",
  "description": "Open-source web maps, powered by Headway and OpenStreetMap",
  "productName": "Headway",
  "author": "Ellen Poe <ellen@ellenhp.me>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format-check": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "fmt": "yarn format",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "jest",
    "build": "yarn tsc",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "dev": "quasar dev"
  },
  "dependencies": {
    "@fragaria/address-formatter": "^3.0.1",
    "@quasar/extras": "^1.0.0",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.3.0",
    "opening_hours": "^3.8.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^2.0.4",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^29.0.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^29.0.2",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.4",
    "vue-tsc": "^1.0.13"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
